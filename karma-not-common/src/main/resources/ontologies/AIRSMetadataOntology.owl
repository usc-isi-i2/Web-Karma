<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:ero="http://www.cubrc.org/ontologies/KDD/Upper/ExtendedRelationOntology#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns="http://www.cubrc.org/ontologies/KDD/Upper/AIRSMetadataOntology#"
  xml:base="http://www.cubrc.org/ontologies/KDD/Upper/AIRSMetadataOntology">
  <owl:Ontology rdf:about="">
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Created with TopBraid Composer</owl:versionInfo>
    <owl:imports rdf:resource="http://purl.org/obo/owl/ro_bfo1-1_bridge"/>
    <owl:imports rdf:resource="http://www.cubrc.org/ontologies/KDD/Upper/ExtendedRelationOntology"/>
  </owl:Ontology>
  <owl:Class rdf:ID="AnnotatePersonNameBearer">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="NamedEntity"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="ObsoleteClass">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Obsolete class</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:ID="Chunking">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="NLP"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="PartOfSpeech">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#NLP"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Tokenizing">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#NLP"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#NLP">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Justification"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="VerbEvent">
    <rdfs:subClassOf rdf:resource="#NLP"/>
  </owl:Class>
  <owl:Class rdf:ID="LexicalDescription">
    <rdfs:subClassOf rdf:resource="#NLP"/>
  </owl:Class>
  <owl:Class rdf:ID="Pedigree">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="ProcessingChainMetadata"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Justification">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#ProcessingChainMetadata"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="SoftwareAgent">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#ProcessingChainMetadata"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Provenance">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#ProcessingChainMetadata"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="AnnotateSentences">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="SentenceBreaking"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Coreference">
    <rdfs:subClassOf rdf:resource="#NLP"/>
  </owl:Class>
  <owl:Class rdf:about="#ProcessingChainMetadata">
    <rdfs:subClassOf rdf:resource="http://www.ifomis.org/bfo/1.1#Entity"/>
  </owl:Class>
  <owl:Class rdf:ID="ProperSequence">
    <rdfs:subClassOf rdf:resource="#NLP"/>
  </owl:Class>
  <owl:Class rdf:about="#NamedEntity">
    <rdfs:subClassOf rdf:resource="#NLP"/>
  </owl:Class>
  <owl:Class rdf:ID="TextZoning">
    <rdfs:subClassOf rdf:resource="#NLP"/>
  </owl:Class>
  <owl:Class rdf:ID="Relationship">
    <rdfs:subClassOf rdf:resource="#NLP"/>
  </owl:Class>
  <owl:Class rdf:ID="AnnotateDataElement">
    <rdfs:subClassOf rdf:resource="#TextZoning"/>
  </owl:Class>
  <owl:Class rdf:about="#SentenceBreaking">
    <rdfs:subClassOf rdf:resource="#NLP"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="assertionReference"/>
  <owl:ObjectProperty rdf:ID="is_mention_of">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >is mention of</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Definition: A instance b1 of a Information Bearing Entity is mention of some instance c1 of some Entity or Event iff b1 is used as a reference to c1.</rdfs:comment>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="is_mentioned_by"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="agent"/>
  <owl:ObjectProperty rdf:ID="merges_into">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Definition: Two or more instances are merged into a single instance iff the multiple instances are determined to be identical, the introduction of new instance is thus a correction of an ontology, replacing two or more identical instances with a single instance.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >merges into</rdfs:label>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="merger_of"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#is_mentioned_by">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >is mentioned by</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Definition: A instance c1 of a Entity or Event is mentioned by an instance b1 of some Information Bearing Entity iff b1 is used as a reference to c1.</rdfs:comment>
    <owl:inverseOf rdf:resource="#is_mention_of"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="agentParams"/>
  <owl:ObjectProperty rdf:about="#merger_of">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Definition: An instance is the merger of two or more instances iff those multiple instances have been determined to be identical, the introduction of the new instance is a correction of an ontology replacing multiple instances of the same entity/event with a single instance.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >merger of</rdfs:label>
    <owl:inverseOf rdf:resource="#merges_into"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="contains_instance_of">
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#Class"/>
          <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#ObjectProperty"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Definition: An Information Bearing Entity b contains_instance_of some Class c or Object Property p iff b contains instance i and i is an instance of c or of p.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >contains instance of</rdfs:label>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="assertionPedigree"/>
  <owl:DatatypeProperty rdf:ID="pedigree"/>
  <owl:DatatypeProperty rdf:ID="confidence"/>
  <owl:DatatypeProperty rdf:ID="justification"/>
  <owl:DatatypeProperty rdf:ID="contains_token">
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#string"/>
          <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
          <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#integer"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Definition: An Information Bearing Entity b1 contains token t iff b1 contains some Information Bearing Entity b2 and b2 has_text_value, or has_integer_value, or has_date_time value t</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >contains token</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="has_token_representation_count">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Definition: A Class c has token representation count n iff there are n-many tokens t such that c has_instance_represented_by_token t. </rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >has token representation count</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="has_instance_represented_by_token">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >has instance represented by token</rdfs:label>
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Definition: A Class c has instance represented by token t iff c has instance i and i is mentioned by Information Bearing Entity b and b has text value t.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <rdf:Description rdf:about="http://www.geneontology.org/formats/oboInOwl#ObsoleteClass">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >A Class for organizing classes that are no longer used</rdfs:comment>
  </rdf:Description>
</rdf:RDF>

